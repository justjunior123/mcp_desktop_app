{
  "name": "mcp-desktop-app",
  "version": "0.1.0",
  "private": true,
  "author": "Your Name",
  "description": "MCP Desktop Application",
  "main": "dist/electron/electron/main.js",
  "scripts": {
    "dev": "npm run clean && npm run kill-ports && npm run db:setup && concurrently \"npm run dev:next\" \"npm run dev:electron\" \"npm run prisma:studio\"",
    "dev:next": "next dev --turbo -p 3002",
    "dev:electron": "tsc -p electron/tsconfig.json && cross-env NODE_ENV=development ELECTRON_HMR=true electron .",
    "build": "next build && tsc -p electron/tsconfig.json",
    "start": "next start -p 3002",
    "lint": "next lint",
    "kill-ports": "node scripts/cleanup.js",
    "clean": "rimraf .next out dist tsconfig.tsbuildinfo electron/dist/electron/tsconfig.tsbuildinfo",
    "build:all": "npm run clean && next build && tsc -p electron/tsconfig.json && electron-builder",
    "db:setup": "prisma generate && prisma migrate deploy",
    "prisma:studio": "dotenv -e .env prisma studio",
    "prisma:migrate": "dotenv -e .env prisma migrate dev",
    "prisma:generate": "dotenv -e .env prisma generate",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.3",
    "@prisma/client": "^6.7.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/classnames": "^2.3.0",
    "@types/cors": "^2.8.18",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "electron": "^28.1.0",
    "electron-updater": "^6.6.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "next": "^15.3.2",
    "prisma": "^6.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-server-dom-webpack": "^19.1.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime-corejs3": "^7.27.1",
    "@electron/remote": "^2.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
